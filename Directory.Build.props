<Project>
  <PropertyGroup>
    <!-- Desativa completamente o fallback do Visual Studio (a raiz do erro) -->
    <DisableImplicitNuGetFallbackFolders>true</DisableImplicitNuGetFallbackFolders>
    
    <!-- ForÃ§a o uso apenas do nuget.org -->
    <RestorePackagesPath>$(UserProfile)\.nuget\packages</RestorePackagesPath>
    <RestoreFallbackFolders>clear</RestoreFallbackFolders>
  </PropertyGroup>
</Project>